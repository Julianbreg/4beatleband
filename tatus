[32m+[m[32m                <code class="big">[m
[32m+[m[32m                    &lt;<span class="red">head</span>&gt;[m
[32m+[m[32m                    <br/><span class="dots">...</span>[m
[32m+[m[32m                    <br/>&lt;<span class="red">link</span> <span class="green">rel</span>=<span class="yellow">"stylesheet"</span> <span class="green">type</span>=<span class="yellow">"text/css"</span> <span class="green">href</span>=<span class="yellow">"your_website_domain/css_root/flaticon.css"</span>&gt;[m
[32m+[m[32m                    <br/><span class="dots">...</span>[m
[32m+[m[32m                    <br/>&lt;/<span class="red">head</span>&gt;[m
[32m+[m[32m                </code>[m
[32m+[m[32m            </li>[m
[32m+[m
[32m+[m[32m            <li>[m
